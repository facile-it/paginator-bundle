{
  "name": "facile-it/paginator-bundle",
  "type": "symfony-bundle",
  "description": "lightweight symfony2 pagination",
  "keywords": [
    "symfony",
    "doctrine",
    "paginator",
    "pagination"
  ],

  "homepage": "http://github.com/facile-it/paginator-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Panina Francesco",
      "email": "francesco.panina@gmail.com"
    }
  ],
  "require": {
    "symfony/framework-bundle": "^2.0 | ^3.0 | ^4.0",
    "doctrine/orm": "^2.2.3",
    "twig/twig": "^1.5 | ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7 | ^5.0",
    "satooshi/php-coveralls": "~1.0"
  },

  "autoload": {
    "psr-4": {
      "Facile\\PaginatorBundle\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "abandoned": "knplabs/knp-paginator-bundle"
}
